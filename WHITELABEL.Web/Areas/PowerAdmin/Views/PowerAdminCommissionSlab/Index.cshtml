@using NonFactors.Mvc.Grid;

@using WHITELABEL.Web.Helper;
@model IEnumerable<WHITELABEL.Data.Models.TBL_WHITE_LEVEL_COMMISSION_SLAB>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/PowerAdmin/Views/Shared/_PowerAdminLayout.cshtml";
}
@Scripts.Render("~/bundles/PowerAdminSlabCommJsDeactivateStatus")
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal padding-top-20">
        <div class="row">
            <div class="col-md-12">
                <div class="panel">
                    <div class="panel-heading">
                        <div class="panel-title" align="right"><a href="@Url.Action("AddCommissionSlab", "PowerAdminCommissionSlab", new { area = "PowerAdmin" })" class="btn btn-info btn-xs text-color-white"><span class="ion-plus"></span>Add New Commission</a></div>
                    </div><!--.panel-heading-->
                    <div class="panel-body">
                        <div class="row example-row">
                            @Html.AjaxGrid(Url.Action("IndexGrid", "PowerAdminCommissionSlab", new { area = "PowerAdmin" }))
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
   
}