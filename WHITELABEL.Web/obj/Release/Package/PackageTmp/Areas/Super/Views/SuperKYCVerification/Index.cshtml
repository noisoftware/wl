@using NonFactors.Mvc.Grid;
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Super/Views/Shared/_SuperLayout.cshtml";
}
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal padding-top-20">
        <div class="row">
            <div class="col-md-12">
                <div class="panel">
                    <div class="panel-body">
                        <div class="row example-row">
                            @Html.AjaxGrid(Url.Action("IndexGrid", "SuperKYCVerification"), new { area = "Super" })
                        </div><!--.row-->
                    </div><!--.panel-body-->
                </div><!--.panel-->
            </div><!--.col-md-12-->
        </div><!--.row-->
    </div>

    @Scripts.Render("~/bundles/SuperKYCVerification");

}